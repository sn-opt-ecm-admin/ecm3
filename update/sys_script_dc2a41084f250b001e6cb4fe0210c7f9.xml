        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	
	gs.include("ORSIntegration");
	
	// 	(current.state==2) WIP
	// 	(current.state==6) RESOLVED
	gs.info("Integration trigger - ORS [ENTER] called for " + current.number + ' state:'+current.state+ ' ors issue id:'+current.ors_issue_id);
	
	var ors = new ORSIntegration();
	if (current.state==2 && current.ors_issue_id.nil()){
		ors.callORS(current, ors.INSERT);
		gs.info("Integration trigger - ORS [INSERT] called for " + current.number);
	}
	else if(current.state==2 && !current.ors_issue_id.nil()){
		ors.callORS(current, ors.UPDATE);
		gs.info("Integration trigger - ORS [UPDATE] called for " + current.number);
	}
	else if (current.state==6 && !current.ors_issue_id.nil()){
		ors.callORS(current, ors.UPDATE);
		gs.info("Integration trigger - ORS [UPDATE] called for " + current.number);
		
	}
	
	
	
	
})(current, previous);]]></script>
